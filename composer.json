{
    "name": "doggy00/laravel-sberbank",
    "description": "Laravel sberbank API client",
    "type": "library",
    "require": {
        "php": "^7.1",
        "guzzlehttp/guzzle": "^6.2",
        "ext-json": "*"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "doggy00",
            "email": "alexandrbozhko88@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "SberbankApi\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SberbankApi\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "SberbankApi\\SberbankApiServiceProvider"
            ]
        }
    },
    "require-dev": {
        "orchestra/testbench": "^6.0"
    }
